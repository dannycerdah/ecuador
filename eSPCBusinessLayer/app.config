</system.data>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="FaceDetectionMainSoap" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:50278/FaceDetectionMain.asmx" binding="basicHttpBinding" bindingConfiguration="FaceDetectionMainSoap" contract="ws.FaceDetectionService.FaceDetectionMainSoap" name="FaceDetectionMainSoap" />
    </client>
  </system.serviceModel>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
